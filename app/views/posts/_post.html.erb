<div class="container">
<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= post.body %>
  
  <br>
   <% post.images.each do |image|%>
    <%= image_tag url_for(image),size:"200x200"%>
  <% end %>

</p>


</div>
</div>