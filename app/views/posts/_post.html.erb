<div class="container">
<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= post.body %>
  </p>
   <% post.images.each do |image|%>
    <%= link_to image_tag(post.picture_as_thumbnail(image)),image %>
  <% end %>
</div>
</div>